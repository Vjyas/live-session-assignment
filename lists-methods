a=[24,'a','pen',345,'government',78,'pen']
a.append(34)
print(a)
b=a.copy()
print(b)
b=a.count('pen')
print(b)
c=[123,'school']
a.extend(c)
print(a)
v=a.index("government")
print(v)
a.insert(4,'vehicle')
print(a)
a.pop()
print(a)
a.remove(78)
print(a)
a.reverse()
print(a)
g=[23,177,45]
g.sort()
print(g)
